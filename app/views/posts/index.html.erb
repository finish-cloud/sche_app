<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>タイトル</th>
      <th>開始日</th>
      <th>終了日</th>
      <th>終日</th>
      <th>スケジュール更新日</th>
      <th>確認</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
    <tr>
      <td><%= post.id %></td>
      <td><%= post.title %></td>
      <td><%= post.start %></td>
      <td><%= post.finish %></td>
      <td><%= post.allday %></td>
      <td><%= post.updated_at %></td>
      <td><%= link_to "確認", post %></td>
      <td><%= %></td>
      <td><%= %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<p><%= link_to "スケジュール新規登録", :new_post %></p>